{
  "name": "portfolio-api",
  "private": true,
  "version": "1.0.0",
  "description": "Portfolio API Repository",
  "license": "MIT",
  "contributors": [
    "Nauris Linde <naurislinde@gmail.com> (https://github.com/FaZeRs)"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "cli:dev": "ts-node -r tsconfig-paths/register src/cli.ts",
    "cli:prod": "node dist/src/cli.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "doc:serve": "npx compodoc -p tsconfig.json -w -s -r 8001",
    "doc:generate": "npx compodoc -p tsconfig.json",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "typeorm": "typeorm-ts-node-commonjs -d ./ormconfig.ts",
    "migration:generate": "npm run typeorm migration:generate",
    "migration:show": "npm run typeorm migration:show",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:create": "typeorm-ts-node-commonjs migration:create"
  },
  "lint-staged": {
    "**/*.ts": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@aws-sdk/abort-controller": "^3.127.0",
    "@aws-sdk/client-s3": "^3.157.0",
    "@aws-sdk/node-http-handler": "^3.127.0",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/bull": "^0.6.1",
    "@nestjs/common": "^9.0.11",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.11",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/swagger": "^6.0.5",
    "@nestjs/typeorm": "^9.0.1",
    "@sentry/node": "^7.11.1",
    "@sentry/tracing": "^7.11.1",
    "argon2": "^0.29.1",
    "aws-sdk": "^2.1202.0",
    "bcrypt": "^5.0.1",
    "bull": "^4.8.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "express": "^4.18.1",
    "handlebars": "^4.7.7",
    "joi": "^17.6.0",
    "lodash": "^4.17.21",
    "multer": "1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.8",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "swagger-ui-express": "^4.5.0",
    "tslib": "^2.4.0",
    "typeorm": "0.3.7",
    "uuid": "^8.3.2",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.127.0",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.11",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^3.15.9",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.13",
    "@types/nodemailer": "^6.4.5",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0"
  },
  "jest": {
    "testResultsProcessor": "jest-sonar-reporter",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coveragePathIgnorePatterns": [
      ".module.ts$"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
