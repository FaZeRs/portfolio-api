{
  "name": "portfolio-api",
  "version": "2.0.1",
  "private": true,
  "packageManager": "pnpm@7.28.0",
  "description": "Portfolio API Repository based on NestJS",
  "license": "MIT",
  "author": "Nauris Linde <naurislinde@gmail.com> (https://github.com/FaZeRs)",
  "homepage": "https://api.naurislinde.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FaZeRs/portfolio-api.git"
  },
  "keywords": [
    "portfolio",
    "nestjs",
    "rest-api"
  ],
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "cli:dev": "ts-node -r tsconfig-paths/register src/cli.ts",
    "cli:prod": "node dist/src/cli.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "doc:serve": "npx compodoc -p tsconfig.json -w -s -r 8001",
    "doc:generate": "npx compodoc -p tsconfig.json",
    "prepare": "node -e \"if (process.env.NODE_ENV !== 'production'){process.exit(1)} \" || husky install",
    "pre-commit": "lint-staged",
    "typeorm": "typeorm-ts-node-commonjs -d ./ormconfig.ts",
    "migration:generate": "npm run typeorm migration:generate",
    "migration:show": "npm run typeorm migration:show",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:create": "typeorm-ts-node-commonjs migration:create",
    "up": "taze major -I"
  },
  "lint-staged": {
    "**/*.ts": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@aws-sdk/abort-controller": "^3.272.0",
    "@aws-sdk/client-s3": "^3.282.0",
    "@aws-sdk/node-http-handler": "^3.282.0",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/bull": "^0.6.3",
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.9",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "@sentry/node": "^7.40.0",
    "@sentry/tracing": "^7.40.0",
    "argon2": "^0.30.3",
    "aws-sdk": "^2.1328.0",
    "bcrypt": "^5.1.0",
    "bull": "^4.10.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "handlebars": "^4.7.7",
    "joi": "^17.8.3",
    "lodash": "^4.17.21",
    "multer": "1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.3.0",
    "rxjs": "^7.8.0",
    "swagger-ui-express": "^4.6.2",
    "tslib": "^2.5.0",
    "typeorm": "0.3.11",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.272.0",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.9",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.14.6",
    "@types/nodemailer": "^6.4.7",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^13.1.2",
    "pnpm": "^7.28.0",
    "prettier": "^2.8.4",
    "supertest": "^6.3.3",
    "taze": "^0.8.5",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0"
  },
  "jest": {
    "testResultsProcessor": "jest-sonar-reporter",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coveragePathIgnorePatterns": [
      ".module.ts$"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
